{
  "name": "applicationinsights",
  "version": "2.0.0-beta.201004.00",
  "description": "Microsoft Application Insights module for Node.js",
  "engines": {
    "node": ">= 8.3.0"
  },
  "keywords": [
    "exception monitoring",
    "request monitoring",
    "performance monitoring",
    "application insights",
    "microsoft",
    "azure",
    "cloud",
    "tracing",
    "telemetry",
    "analytics",
    "apm"
  ],
  "bugs": "https://github.com/microsoft/ApplicationInsights-node.js/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/ApplicationInsights-node.js"
  },
  "license": "MIT",
  "contributors": [
    {
      "name": "Application Insights Developer Support",
      "email": "aidevsupport@microsoft.com"
    },
    {
      "name": "Application Insights SDK Maintainers",
      "email": "appinsightssdk@microsoft.com"
    }
  ],
  "main": "./out/applicationinsights.js",
  "types": "./out/applicationinsights.d.ts",
  "scripts": {
    "backcompattest": "npm run build && npm pack && node --use_strict ./Tests/BackCompatibility/RunBackCompatTests.js",
    "build": "npm run compile",
    "clean": "rm -rf ./out && rm -rf ./node_modules",
    "lint": "eslint . --ext .ts",
    "compile": "tsc --project ./tsconfig.json",
    "functionaltest": "npm run build && npm pack && node --use_strict ./Tests/FunctionalTests/RunFunctionalTests.js",
    "prepublishOnly": "npm run build",
    "test": "ts-mocha -p tsconfig.json './Tests/{**/*.tests.ts, *.tests.ts}' --recursive --no-exit",
    "test:debug": "npm run test -- --inspect-brk"
  },
  "dependencies": {
    "@azure/monitor-opentelemetry-exporter": "^1.0.0-preview.6",
    "@opentelemetry/api": "^0.12.0",
    "@opentelemetry/context-async-hooks": "0.12.0",
    "@opentelemetry/context-base": "^0.12.0",
    "@opentelemetry/core": "^0.12.0",
    "@opentelemetry/node": "^0.12.0",
    "@opentelemetry/plugins-node-all": "^0.11.0",
    "@opentelemetry/semantic-conventions": "^0.12.0",
    "@opentelemetry/tracing": "^0.12.0",
    "diagnostic-channel": "^0.3.1",
    "diagnostic-channel-publishers": "^0.4.2"
  },
  "devDependencies": {
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.14.6",
    "@types/sinon": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-no-only-tests": "^2.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.1.1",
    "mocha": "^8.2.1",
    "prettier": "^2.1.2",
    "sinon": "^9.2.1",
    "ts-mocha": "^8.0.0",
    "typescript": "^4.0.5"
  },
  "publishConfig": {
    "tag": "beta"
  }
}
